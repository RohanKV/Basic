1. Write a program to print sum of n numbers.
#include<stdio.h>
void main()
{
  int a[100],n,i,sum = 0;
  printf("Enter the length of numbers:");
  scanf("%d",&n);
  printf(" \n Enter the numbers to be summed: ");
  for(i=0;i<n;i++){
    scanf("%d",a[i]);
  }
  for(i=0;i<n;i++){
    sum = sum+a[i];
  }
  printf("\n Sum: %d ",sum);
}

USING STRUCTURE:

#include<stdio.h>
struct mem
{
    int length;
    int a[100];
    int sum;
}s;

int input()
{
    struct s;
    int i;
    printf("Enter no of elements:");
    scanf("%d",&s.length);
    printf("\n Enter the elements:");
    for(i=0;i<s.length;i++){
        scanf("%d",&s.a[i]);
    }
}

int compute(int length, int a[])
{
    struct s;
    int i;
    for(i=0;i<length;i++){
        s.sum = s.sum + a[i];
    } 
}

void output(int s)
{
    printf("The Sum is: %d", s);
}

void main()
{
    struct s;
    input();
    compute(s.length, s.a);
    output(s.sum);  
}


2.Write function to get n numbers from the user.
int input()
{
    int i, length, a[100];
    printf("Enter no of elements:");
    scanf("%d",&length);
    printf("\n Enter the elements:");
    for(i=0;i< length;i++){
        scanf("%d",&a[i]);
    }
}

3. Write function to compute the sum of n numbers.
int compute(int length, int a[])
{
    int i, sum = 0;
    for(i=0;i<length;i++){
        sum = sum + a[i];
    }
    return sum;
}

4. Write a program to find the sum of square roots of n numbers.
#include<stdio.h>
#include<math.h>
struct mem
{
    int length;
    int a[100];
    int sum;
}s;

int input()
{
    struct s;
    int i;
    printf("Enter no of elements:");
    scanf("%d",&s.length);
    printf("\n Enter the elements:");
    for(i=0;i<s.length;i++){
        scanf("%d",&s.a[i]);
    }
}

int compute(int length, int a[])
{
    struct s;
    int i;
    for(i=0;i<length;i++){
        a[i] = sqrt(a[i]);
    }
    for(i=0;i<length;i++){
        s.sum = s.sum + a[i];
    } 
}

void output(int s)
{
    printf("The Sum is: %d", s);
}

void main()
{
    struct s;
    input();
    compute(s.length, s.a);
    output(s.sum);
    
}
